{
  "Statement": [
    {
      "Action": "*",
      "Condition": {
        "ArnNotLike": {
          "aws:PrincipalARN": [
            "arn:aws:iam::*:role/${ORG_ADMIN_ROLE}",
            "arn:aws:iam::*:role/aws*",
            "arn:aws:iam::*:role/${ACCELERATOR_PREFIX}*"
          ]
        }
      },
      "Effect": "Deny",
      "Resource": "*",
      "Sid": "DenyAllAWSServicesExceptBreakglassRoles"
    }
  ],
  "Version": "2012-10-17"
}